<h1>Find Students<h1>

<%= form_tag students_path, method: :get do %>
  <%= text_field_tag :search, value = "Enter Student Name" %>
  <%= submit_tag "Submit" %>
<% end %>

<% if @students.present? %>
  <% @students.each do |student| %>
    <%= render partial: 'student', locals: {student: student} %>
  <% end %>
<% end %>